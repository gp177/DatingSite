{% extends "master.html.twig" %}


{% block headextra %}Profile{% endblock %}

  
   {% block content %}
            {% if errorList %}
                <ul>
                    {% for error in errorList %}
                        <li>{{error}}</li>
                        {% endfor %}
                </ul>
            {% endif %}
      
           
       <img style="width:200px; height: 200px;" src="{{pl.profPicId}}" alt="profile picture" />
       <p><span>Name:</span><span>{{pl.name}}</span></p>
       <p><span>Birth Date:</span><span>{{pl.birthDate}}</span></p>
       <p><span>Country:</span><span>{{pl.country}}</span></p>
       <p><span>City:</span><span>{{pl.city}}</span></p>
       <p><span>Language:</span><span>{{pl.language}}</span></p>
       <p><span>Additional Info:</span><span>{{pl.additionalInfo}}</span></p>
       {% endblock %}{# empty Twig template #}
